    "rollup-plugin-visualizer": "^5.8.2",
    "sass": "^1.55.0",
    "stylus": "^0.59.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
